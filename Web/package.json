    "@tanstack/react-query": "^5.90.7",
    "@types/leaflet": "^1.9.21",
    "axios": "^1.13.2",
    "country-state-city": "^3.2.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "framer-motion": "^12.23.24",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.9.5",
    "react-toastify": "^11.0.5",
    "tailwindcss": "^4.1.15"
  },
  "devDependencies": {
     "@eslint/js": "^9.36.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
